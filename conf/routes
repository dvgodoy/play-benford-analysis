# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET        /                    controllers.Benf.PageIndex
GET        /count               controllers.Benf.count
GET        /list                controllers.Benf.list
GET        /sum/:n              controllers.Benf.sum(n)

GET        /root                controllers.Application.root
GET        /remote              controllers.Application.rootremote
GET        /carscounter         controllers.Benf.carscounter

GET        /rootJson            controllers.Application.rootJson
GET        /remoteJson          controllers.Application.remoteJson

# Home page

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
